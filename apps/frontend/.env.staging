# API Configuration
VITE_API_URL=https://your-staging-api.railway.app

# Authentication
VITE_JWT_SECRET=staging-jwt-secret

# Email Configuration
VITE_EMAIL_SERVICE_URL=https://your-staging-api.railway.app/api/email

# Payment Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_staging_stripe_key_here

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-STAGING-ID

# Monitoring
VITE_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id

# Environment
VITE_ENVIRONMENT=staging
VITE_NODE_ENV=production

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_MONITORING=true
VITE_ENABLE_SENTRY=true

# App Configuration
VITE_APP_NAME="Club+ (Staging)"
VITE_APP_VERSION=1.0.0
